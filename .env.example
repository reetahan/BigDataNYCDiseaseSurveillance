# Reddit API Credentials
REDDIT_CLIENT_ID=""
REDDIT_CLIENT_SECRET=""
REDDIT_USER_AGENT=HealthScraper/1.0 by anonymous

# Kafka Configuration (using your Docker setup)
KAFKA_BOOTSTRAP_SERVERS=localhost:9092

# Bluesky Scraper Configuration

# Bluesky Authentication (required for API access)
BLUESKY_HANDLE=""
BLUESKY_PASSWORD=""
# Scraper Settings
SCRAPE_INTERVAL=300
MAX_POSTS_PER_SEARCH=100
OUTPUT_FILE=data/bluesky/bluesky_posts.json

# Custom Keywords (comma-separated)
CUSTOM_HEALTH_KEYWORDS=
CUSTOM_NYC_KEYWORDS=

# Rate Limiting
RATE_LIMIT_DELAY=2

# Logging

LOG_LEVEL=INFO

# AWS configuration
AWS_ACCESS_KEY_ID=""
AWS_SECRET_ACCESS_KEY=""
AWS_BUCKET_NAME=""
AWS_REGION=us-east-1
